	file = {Snapshot:/home/claire/Zotero/storage/AJ2AMYCG/98B9BF3147B12953495C56CC9FF38701.html:text/html;Full Text PDF:/home/claire/Zotero/storage/73AU8QBZ/Davaille and Jaupart - 1993 - Transient high-Rayleigh-number thermal convection .pdf:application/pdf},
