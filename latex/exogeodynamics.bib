@article{nimmoModellingPlumerelatedUplift1996,
	title = {Modelling plume-related uplift, gravity and melting on {Venus}},
	volume = {145},
	issn = {0012-821X},
	url = {http://www.sciencedirect.com/science/article/pii/S0012821X96002002},
	doi = {10.1016/S0012-821X(96)00200-2},
	abstract = {Three roughly circular regions on Venus about 1000 km across are identified as potential Hawaii-scale plume sites on the basis of their gravity (70–90 mgal) and topography (1.6–2.0 km) anomalies, and signs of melt generation and rifting. Axisymmetric isoviscous convection models are used to reproduce gravity profiles across these plumes and the line-of-sight acceleration of the Magellan spacecraft as it passes over them. The best fitting models have a conductive lid thickness of less than 150 km, mantle viscosity of 8.9 × 1019 to 9.6 × 1020 Pa s and a basal heat flux of 15–25 m W m−2. The lid thickness is constrained by requiring a modest amount of melt generation and a potential temperature of about 1300°C. The high mantle viscosity relative to that of Earth is probably a consequence of the absence of water in the mantle, and may help to explain the current absence of plate tectonics on Venus. The low heat flux suggests that the thermal evolution of Venus has differed from that of the Earth.},
	language = {en},
	number = {1},
	urldate = {2020-04-18},
	journal = {Earth and Planetary Science Letters},
	author = {Nimmo, Francis and McKenzie, Dan},
	month = dec,
	year = {1996},
	keywords = {volcanism, Venus, convection, mantle, viscosity, lithosphere, gravity anomalies, heat flow, hot spots, Magellan Program, models, topography},
	pages = {109--123},
	file = {ScienceDirect Full Text PDF:/home/claire/Zotero/storage/AKZSYRWX/Nimmo and McKenzie - 1996 - Modelling plume-related uplift, gravity and meltin.pdf:application/pdf;ScienceDirect Snapshot:/home/claire/Zotero/storage/T6XTPGE4/S0012821X96002002.html:text/html},
}

@article{kieferDynamicModelVenus1986,
	title = {A dynamic model of {Venus}'s gravity field},
	volume = {13},
	copyright = {Copyright 1986 by the American Geophysical Union.},
	issn = {1944-8007},
	url = {https://agupubs.onlinelibrary.wiley.com/doi/abs/10.1029/GL013i001p00014},
	doi = {10.1029/GL013i001p00014},
	abstract = {Unlike on Earth, long-wavelength gravity anomalies and topography correlate well on Venus. Venus's admittance curve from spherical harmonic degree 2 to 18 is inconsistent with either Airy or Pratt isostasy but is consistent with dynamic support from mantle convection. A model using whole mantle flow and a high viscosity near-surface layer overlying a constant viscosity mantle reproduces this admittance curve. On Earth, the effective viscosity deduced from geoid modeling increases by a factor of 300 from the asthenosphere to the lower mantle. These viscosity estimates may be biased by neglect of lateral variations in mantle viscosity. The different effective viscosity profiles for Earth and Venus may reflect their convective styles, with tectonism and mantle heat transport dominated by hot plumes on Venus and by subducted slabs on Earth.},
	language = {en},
	number = {1},
	urldate = {2020-04-18},
