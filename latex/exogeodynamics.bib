	keywords = {dynamic topography, isostasy, Venus, venus, Crustal thickness, Dynamic topography, Gravity, Inversion},
	pages = {24--31},
	file = {ScienceDirect Full Text PDF:/home/claire/Zotero/storage/2JMXNMID/Yang et al. - 2016 - Separation of dynamic and isostatic components of .pdf:application/pdf;ScienceDirect Snapshot:/home/claire/Zotero/storage/XSV5CBP6/S0032063315301495.html:text/html},
}

