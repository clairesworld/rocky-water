@article{kunimotoSearchingEntiretyKepler2020,
	title = {Searching the {Entirety} of {Kepler} {Data}. {II}. {Occurrence} {Rate} {Estimates} for {FGK} {Stars}},
	url = {http://arxiv.org/abs/2004.05296},
	abstract = {We present exoplanet occurrence rates estimated with approximate Bayesian computation for planets with radii between 0.5-16 \$R\_\{{\textbackslash}bigoplus\}\$ and orbital periods between 0.78-400 days, orbiting FGK dwarf stars. We base our results on an independent planet catalogue compiled from our search of all {\textasciitilde}200,000 stars observed over the Kepler mission, with precise planetary radii supplemented by Gaia DR2-incorporated stellar radii. We take into account detection and vetting efficiency, planet radius uncertainty, and reliability against transit-like noise signals in the data. By analyzing our FGK occurrence rates as well as those computed after separating F, G, and K type stars, we explore dependencies on stellar effective temperature, planet radius, and orbital period. We reveal new characteristics of the photoevaporation-driven "radius gap" between {\textasciitilde}1.5-2 \$R\_\{{\textbackslash}bigoplus\}\$, indicating that the bimodal distribution previously revealed for \$P\$ {\textless} 100 days exists only over a much narrower range of orbital periods, above which sub-Neptunes dominate and below which super-Earths dominate. Finally, we provide several estimates of the "eta-Earth" value -- the frequency of potentially habitable, rocky planets orbiting Sun-like stars. For planets with sizes 0.75 - 1.5 \$R\_\{{\textbackslash}bigoplus\}\$ orbiting in a conservatively defined Habitable Zone (0.99 - 1.70 AU) around G type stars, we place an upper limit (84.1th percentile) of {\textless}0.18 planets per star.},
	urldate = {2020-04-14},
	journal = {arXiv:2004.05296 [astro-ph]},
	author = {Kunimoto, Michelle and Matthews, Jaymie M.},
	month = apr,
	year = {2020},
	note = {arXiv: 2004.05296},
	keywords = {Astrophysics - Earth and Planetary Astrophysics, Astrophysics - Solar and Stellar Astrophysics},
	file = {arXiv.org Snapshot:/home/claire/Zotero/storage/ZNEEH2PH/2004.html:text/html;arXiv Fulltext PDF:/home/claire/Zotero/storage/DKUVP8CZ/Kunimoto and Matthews - 2020 - Searching the Entirety of Kepler Data. II. Occurre.pdf:application/pdf},
