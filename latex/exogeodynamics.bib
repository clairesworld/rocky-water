@article{phillipsAncientGeodynamicsGlobalscale2001,
	title = {Ancient geodynamics and global-scale hydrology on {Mars}},
	volume = {291},
	doi = {10.1126/science.1058701},
	abstract = {Loading of the lithosphere of Mars by the Tharsis rise explains much of the global shape and long-wavelength gravity field of the planet, including a ring of negative gravity anomalies and a topographic trough around Tharsis, as well as gravity anomaly and topographic highs centered in Arabia Terra and extending northward toward Utopia. The Tharsis-induced trough and antipodal high were largely in place by the end of the Noachian Epoch and exerted control on the location and orientation of valley networks. The release of carbon dioxide and water accompanying the emplacement of ∼3 × 108 cubic kilometers of Tharsis magmas may have sustained a warmer climate than at present, enabling the formation of ancient valley networks and fluvial landscape denudation in and adjacent to the large-scale trough.},
	number = {5513},
	journal = {Science},
	author = {Phillips, R.J. and Zuber, M.T. and Solomon, S.C. and Golombek, M.P. and Jakosky, B.M. and Banerdt, W.B. and Smith, D.E. and Williams, R.M.E. and Hynek, B.M. and Aharonson, O. and Hauck II, S.A.},
