	file = {Snapshot:/home/claire/Zotero/storage/CZK6JXQG/19273.html:text/html;Full Text PDF:/home/claire/Zotero/storage/KCBTCW92/Petigura et al. - 2013 - Prevalence of Earth-size planets orbiting Sun-like.pdf:application/pdf},
}

