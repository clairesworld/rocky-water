	month = apr,
	year = {2010},
	pmid = {20378775},
	keywords = {dynamic topography, venus},
	pages = {605--608},
	file = {Snapshot:/home/claire/Zotero/storage/QKBCNJZA/605.html:text/html;Full Text PDF:/home/claire/Zotero/storage/N6T7AL6V/Smrekar et al. - 2010 - Recent Hotspot Volcanism on Venus from VIRTIS Emis.pdf:application/pdf},
