@article{zengSimpleAnalyticalModel2017,
	title = {A {Simple} {Analytical} {Model} for {Rocky} {Planet} {Interiors}},
	volume = {837},
	issn = {1538-4357},
	url = {http://arxiv.org/abs/1606.03522},
	doi = {10.3847/1538-4357/aa6218},
	abstract = {This work aims at exploring the scaling relations among rocky exoplanets. With the assumption of internal gravity increasing linearly in the core, and staying constant in the mantle, and tested against numerical simulations, a simple model is constructed, applicable to rocky exoplanets of core mass fraction (CMF) \${\textbackslash}in 0.1{\textbackslash}sim0.4\$ and mass \${\textbackslash}in 0.1 {\textbackslash}sim 10 M\_\{{\textbackslash}oplus\}\$. Various scaling relations are derived: (1) core radius fraction \${\textbackslash}rm CRF {\textbackslash}approx {\textbackslash}sqrt\{CMF\}\$, (2) Typical interior pressure \$P\_\{{\textbackslash}text\{typical\}\} {\textbackslash}sim g\_\{{\textbackslash}rm\{s\}\}{\textasciicircum}2\$ (surface gravity squared), (3) core formation energy \$E\_\{{\textbackslash}rm diff\} {\textbackslash}sim {\textbackslash}frac\{1\}\{10\} E\_\{{\textbackslash}rm grav\}\$ (the total gravitational energy), (4) effective heat capacity of the mantle \$C\_\{{\textbackslash}rm p\}{\textbackslash}approx {\textbackslash}left( {\textbackslash}frac\{M\_\{{\textbackslash}rm\{p\}\}\}\{M\_\{{\textbackslash}oplus\}\} {\textbackslash}right) {\textbackslash}cdot 7 {\textbackslash}cdot 10{\textasciicircum}\{27\}\$ J K\${\textasciicircum}\{-1\}\$, and (5) the moment of inertia \$I{\textbackslash}approx {\textbackslash}frac\{1\}\{3\} {\textbackslash}cdot M\_\{{\textbackslash}rm\{p\}\} {\textbackslash}cdot R\_\{{\textbackslash}rm\{p\}\}{\textasciicircum}2\$. These scaling relations, though approximate, are handy for quick use owing to their simplicity and lucidity, and provide insights into the interior structures of rocky exoplanets. As examples, this model is applied to several planets including Earth, GJ 1132b, Kepler-93b, and Kepler-20b, and made comparison with the numerical method.},
	number = {2},
	urldate = {2020-01-13},
