@article{bindiDiscoveryElgoresyiteMg2021,
	title = {Discovery of {Elgoresyite}, ({Mg},{Fe}){5Si2O9}: {Implications} for {Novel} {Iron}-{Magnesium} {Silicates} in {Rocky} {Planetary} {Interiors}},
	volume = {5},
	shorttitle = {Discovery of {Elgoresyite}, ({Mg},{Fe}){5Si2O9}},
	url = {https://doi.org/10.1021/acsearthspacechem.1c00157},
	doi = {10.1021/acsearthspacechem.1c00157},
	abstract = {As the most abundant material of rocky planets, high-pressure polymorphs of iron- and aluminum-bearing magnesium silicates have long been sought by both observations and experiments. Meanwhile, it was recently revealed that iron oxides form (FeO)m(Fe2O3)n homologous series above ∼10 GPa according to laboratory high-pressure experiments. Here, we report a new high-pressure iron-magnesium silicate, recently approved by the International Mineralogical Association as a new mineral (No. 2020-086) and named elgoresyite, in a shock-induced melt vein of the Suizhou L6 chondrite with a chemistry of (Mg,Fe)5Si2O9. The crystal structure of this new silicate is the same as the iron oxide Fe7O9, strongly suggesting that silicates also form ((Mg,Fe)O)m + n(SiO2)n series that are isostructural to iron oxides via (Mg2+,Fe2+) + Si4+ = 2Fe3+ substitution. To test this hypothesis, the phase relationships of the silicates and iron oxides should be further investigated at higher temperature conditions. Newly found iron-magnesium silicate is a potential constituent mineral in rocky planets with relatively high MgO + FeO content.},
	number = {8},
	urldate = {2021-08-30},
	journal = {ACS Earth and Space Chemistry},
	author = {Bindi, Luca and Sinmyo, Ryosuke and Bykova, Elena and Ovsyannikov, Sergey V. and McCammon, Catherine and Kupenko, Ilya and Ismailova, Leyla and Dubrovinsky, Leonid and Xie, Xiande},
	month = aug,
	year = {2021},
	note = {Publisher: American Chemical Society},
	pages = {2124--2130},
	file = {Full Text PDF:/home/claire/Zotero/storage/NQC9ZXLG/Bindi et al. - 2021 - Discovery of Elgoresyite, (Mg,Fe)5Si2O9 Implicati.pdf:application/pdf;ACS Full Text Snapshot:/home/claire/Zotero/storage/2TRD32U3/acsearthspacechem.html:text/html},
}

@article{ballmerDiversityExoplanetsInterior2021,
	title = {The {Diversity} of {Exoplanets}: {From} {Interior} {Dynamics} to {Surface} {Expressions}},
	shorttitle = {The {Diversity} of {Exoplanets}},
	url = {http://arxiv.org/abs/2108.08385},
	abstract = {The coupled interior-atmosphere system of terrestrial exoplanets remains poorly understood. Exoplanets show a wide variety of sizes, densities, surface temperatures, and interior structures, with important knock-on effects for this coupled system. Many exoplanets are predicted to have a "stagnant lid" at the surface, with a rigid stationary crust, sluggish mantle convection, and only minor volcanism. However, if exoplanets have Earth-like plate tectonics, which involves several discrete, slowly moving plates and vigorous tectono-magmatic activity, then this may be critical for planetary habitability and have implications for the development (and evolution) of life in the galaxy. Here, we summarize our current knowledge of coupled planetary dynamics in the context of exoplanet diversity.},
	urldate = {2021-08-30},
	journal = {arXiv:2108.08385 [astro-ph]},
	author = {Ballmer, Maxim D. and Noack, Lena},
	month = aug,
	year = {2021},
	note = {arXiv: 2108.08385},
	keywords = {Astrophysics - Earth and Planetary Astrophysics},
	file = {arXiv Fulltext PDF:/home/claire/Zotero/storage/YJRZVJWP/Ballmer and Noack - 2021 - The Diversity of Exoplanets From Interior Dynamic.pdf:application/pdf;arXiv.org Snapshot:/home/claire/Zotero/storage/UT4ZSU65/2108.html:text/html},
}

@article{xuExogeologyPollutedWhite2021,
	title = {Exogeology from {Polluted} {White} {Dwarfs}},
	url = {http://arxiv.org/abs/2108.08384},
	abstract = {It is difficult to study the interiors of terrestrial planets in the Solar System and the problem is magnified for distant exoplanets. However, sometimes nature is helpful. Some planetary bodies are torn to fragments and consumed by the strong gravity close to the descendants of Sun-like stars, white dwarfs. We can deduce the general composition of the planet when we observe the spectroscopic signature of the white dwarf. Most planetary fragments that fall into white dwarfs appear to be rocky with a variable fraction of associated ice and carbon. These white dwarf planetary systems provide a unique opportunity to study the geology of exoplanetary systems.},
	urldate = {2021-08-30},
	journal = {arXiv:2108.08384 [astro-ph]},
	author = {Xu, Siyi and Bonsor, Amy},
	month = aug,
	year = {2021},
	note = {arXiv: 2108.08384},
	keywords = {Astrophysics - Earth and Planetary Astrophysics, Astrophysics - Solar and Stellar Astrophysics},
	file = {arXiv Fulltext PDF:/home/claire/Zotero/storage/AZVF8R6Y/Xu and Bonsor - 2021 - Exogeology from Polluted White Dwarfs.pdf:application/pdf;arXiv.org Snapshot:/home/claire/Zotero/storage/TSNXUASX/2108.html:text/html},
}

@article{putirkaCompositionalDiversityRocky2021,
	title = {Compositional {Diversity} of {Rocky} {Exoplanets}},
	url = {http://arxiv.org/abs/2108.08383},
	abstract = {Star compositions are essential for examining densities and compositional ranges of rocky exoplanets, testing their similarity to Earth. Stellar elemental abundances and planetary orbital data show that of the {\textasciitilde}5000 known minerals, exoplanetary silicate mantles will contain mostly olivine, orthopyroxene, and clinopyroxene, \${\textbackslash}pm\$ quartz, and magnesiuwustite at the extremes; wholly exotic mineralogies are likely absent. Understanding these exotic geological systems requires a better marriage of geological insights to astronomical data. The study of exoplanets is like a mirror, reflecting our incomplete understanding of Earth and neighboring planets; new geological/planetary experiments, informed by exoplanet studies, are needed for effectual progress.},
	urldate = {2021-08-30},
	journal = {arXiv:2108.08383 [astro-ph, physics:physics]},
	author = {Putirka, Keith and Dorn, Caroline and Hinkel, Natalie and Unterborn, Cayman},
	month = aug,
	year = {2021},
	note = {arXiv: 2108.08383},
	keywords = {Astrophysics - Earth and Planetary Astrophysics, Astrophysics - Solar and Stellar Astrophysics, Physics - Geophysics},
	file = {arXiv Fulltext PDF:/home/claire/Zotero/storage/HKICVD4V/Putirka et al. - 2021 - Compositional Diversity of Rocky Exoplanets.pdf:application/pdf;arXiv.org Snapshot:/home/claire/Zotero/storage/ZNAIN274/2108.html:text/html},
}

@article{shorttleWhyGeosciencesExoplanetary2021,
	title = {Why {Geosciences} and {Exoplanetary} {Sciences} {Need} {Each} {Other}},
	url = {http://arxiv.org/abs/2108.08382},
	abstract = {The study of planets outside our solar system may lead to major advances in our understanding of the Earth, and provide insight into the universal set of rules by which planets form and evolve. To achieve these goals requires applying geoscience's wealth of Earth observations to fill in the blanks left by the necessarily minimalist exoplanetary observations. In turn, Earth's many one-offs, e.g., plate tectonics, surface liquid water, a large moon, and life - which have long presented chicken and egg type conundrums for geoscientists - may find resolution in the study of exoplanets possessing only a subset of these phenomena.},
	urldate = {2021-08-30},
	journal = {arXiv:2108.08382 [astro-ph, physics:physics]},
	author = {Shorttle, Oliver and Hinkel, Natalie and Unterborn, Cayman},
	month = aug,
	year = {2021},
	note = {arXiv: 2108.08382},
	keywords = {Astrophysics - Earth and Planetary Astrophysics, Physics - Geophysics},
	file = {arXiv Fulltext PDF:/home/claire/Zotero/storage/M3T99S9G/Shorttle et al. - 2021 - Why Geosciences and Exoplanetary Sciences Need Eac.pdf:application/pdf;arXiv.org Snapshot:/home/claire/Zotero/storage/B3U3RP5R/2108.html:text/html},
}

